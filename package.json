{
  "name": "chess-engine",
  "version": "1.0.0",
  "description": "A modern chess application with AI analysis built with React and Node.js",
  "main": "index.js",
  "scripts": {
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && node server.js",
    "start:frontend": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\""
  },
  "keywords": [
    "chess",
    "react",
    "nodejs",
    "typescript",
    "ai",
    "game"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/chess-engine.git"
  },
  "bugs": {
